# Ubuntu
sudo apt-get install zsh python3-pip virtualenv linuxbrew-wrapper -y
# Brew taps
brew install jesseduffield/lazygit/lazygit
# Brew Installs
brew install thefuck neovim lazygit awscli pyenv pyenv-virtualenvwrapper pre-commit  git-secrets 

# pip installs
pip3 install virtualenvwrapper


